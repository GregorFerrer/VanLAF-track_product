<odoo>
  <data>
    <record model="ir.ui.view" id="track_product.cost_product_tree">
      <field name="name">track_product.cost_product_tree</field>
      <field name="model">track.costproduct</field>
      <field name="arch" type="xml">
        <tree>
            <field name="create_date" string="Date"/>
            <field name="name"/>
            <field name="product_tmpl"/>
            <field name="value"/>
        </tree>
      </field>
    </record>


    <record model="ir.actions.act_window" id="track_product.action_window_track_cost_product">
      <field name="name">track cost product</field>
      <field name="res_model">track.costproduct</field>
      <field name="view_mode">tree</field>
    </record>


  </data>
</odoo>